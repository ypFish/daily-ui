.com-navigation {
  position: relative;
  width: 100vw;
  z-index: 50;
}
.com-navigation .navbar {
  width: 100vw;
  background-color: #fff;
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 100;
  transform: rotate(0deg);
}
.com-navigation .navbar .content {
  position: absolute;
  display: flex;
  align-items: center;
}
.com-navigation .navbar .content .icon-box {
  display: flex;
  border: 1rpx solid rgba(0, 0, 0, 0.1);
  align-items: center;
  overflow: hidden;
}
.com-navigation .navbar .content .icon-box .icon-button {
  flex: 1;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
@keyframes circleAns {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(720deg);
  }
}
.com-navigation .navbar .content .icon-box .icon-button .circleAns {
  animation: circleAns 2s linear 0s infinite;
}
.com-navigation .navbar .content .icon-box .icon-button-tap {
  background-color: rgba(0, 0, 0, 0.2);
}
.com-navigation .navbar .content .icon-box .seq-line {
  width: 1rpx;
  height: 18px;
  background-color: rgba(180, 180, 180, 0.6);
}
.com-navigation .navbar .content .title {
  color: #000;
  flex: 1;
  text-align: center;
  font-weight: bold;
}
