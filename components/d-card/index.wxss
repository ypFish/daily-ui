.sline {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.number-font {
  letter-spacing: 2rpx;
  font-family: "Helvetica", "SimSun", "PingFang SC", "Microsoft YaHei";
}
.card-title-font {
  font-size: 33rpx;
  line-height: 46rpx;
}
.big-font {
  font-size: 30rpx;
  line-height: 44rpx;
}
.medium-font {
  font-size: 28rpx;
  line-height: 40rpx;
}
.small-font {
  font-size: 26rpx;
  line-height: 36rpx;
}
.tiny-font {
  font-size: 20rpx;
  line-height: 26rpx;
}
.line-block {
  width: 100%;
  padding: 20rpx;
  background-color: rgba(0, 0, 0, 0.05);
  border-radius: 12rpx;
}
.light-line-block {
  width: 100%;
  padding: 20rpx;
  background-color: rgba(0, 0, 0, 0.03);
  border-radius: 12rpx;
}
.tip-block {
  white-space: nowrap;
  display: inline-block;
  font-size: 20rpx;
  line-height: 26rpx;
  padding: 5rpx 8rpx;
  border-radius: 6rpx;
}
.com-card {
  width: 100%;
  background-color: #fff;
  padding: 30rpx;
  border-radius: 16rpx;
  box-shadow: 0 0 10rpx rgba(51, 51, 51, 0.05);
  overflow: hidden;
  position: relative;
}
.com-card .archor {
  position: absolute;
  width: 0rpx;
  height: 0rpx;
  z-index: -1;
  left: 0;
  top: -200rpx;
}
.com-card .head-box {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: row;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid rgba(114, 121, 134, 0.2);
}
.com-card .head-box .icon-box {
  width: 84rpx;
  height: 84rpx;
  background-color: #16a085;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  border-radius: 12rpx;
}
.com-card .head-box .icon-box .icon {
  transition: opacity 0.3s ease 0s;
}
.com-card .head-box .title-box {
  flex: 1;
  height: 86rpx;
  padding: 0 16rpx;
  overflow: hidden;
}
.com-card .head-box .title-box .title {
  color: #444444;
  font-weight: bold;
  font-size: 33rpx;
  line-height: 46rpx;
  min-width: 60rpx;
}
.com-card .head-box .title-box .title .icon {
  display: inline-block;
  margin-left: 10rpx;
  transition: opacity 0.3s ease 0s;
  opacity: 1;
  vertical-align: -2rpx;
}
.com-card .head-box .title-box .desc {
  color: #727986;
  font-weight: bold;
  font-size: 28rpx;
  line-height: 40rpx;
  margin-top: 2rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.com-card .head-box .nav-box {
  height: 86rpx;
  width: 100rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  overflow: hidden;
}
.com-card .head-box .nav-box .nav-share-button {
  width: 100%;
  height: 86rpx;
  background-color: #FFFFFF;
  color: #16a085;
  display: flex;
  font-weight: bold;
  align-items: flex-end;
  flex-direction: column;
  text-align: right;
  padding: 0;
}
.com-card .head-box .nav-box .nav-share-button .button-icon {
  display: inline-block;
  text-align: right;
  margin-right: 8rpx;
}
.com-card .head-box .nav-box .nav-share-button .button-text {
  display: block;
  font-size: 26rpx;
  line-height: 36rpx;
  width: 100rpx;
  margin: 4rpx;
}
.com-card .gap-line-hidden {
  border-bottom: none;
  padding-bottom: 0;
}
.com-card .content-box {
  width: 100%;
  min-height: 300rpx;
  padding-top: 20rpx;
}
.com-card-empty .content-box {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  height: 420rpx;
}
.com-card-empty .content-box .empty-img {
  display: block;
  width: 220rpx;
  height: 220rpx;
  margin-top: -20rpx;
}
.com-card-empty .content-box .empty-text {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #727986;
  font-weight: bold;
}
.com-card-loading .head-box-loading .icon-box {
  background-color: rgba(114, 121, 134, 0.2);
}
.com-card-loading .head-box-loading .icon-box .icon {
  opacity: 0;
}
.com-card-loading .head-box-loading .title-box .title {
  width: 20%;
  background-color: rgba(114, 121, 134, 0.2);
  border-radius: 12rpx;
  overflow: hidden;
  color: rgba(0, 0, 0, 0);
  height: 34rpx;
  margin-bottom: 18rpx;
}
.com-card-loading .head-box-loading .title-box .title .icon {
  opacity: 0;
}
.com-card-loading .head-box-loading .title-box .desc {
  width: 80%;
  height: 34rpx;
  background-color: rgba(114, 121, 134, 0.2);
  border-radius: 12rpx;
  overflow: hidden;
  color: rgba(0, 0, 0, 0);
}
.com-card-loading .head-box-loading .nav-box {
  opacity: 0;
}
.com-card-loading .content-box {
  height: 600rpx;
  position: relative;
  padding-top: 30rpx;
}
.com-card-loading .content-box .loading-line {
  display: inline-block;
  width: 100%;
  background-color: rgba(114, 121, 134, 0.2);
  height: 56rpx;
  border-radius: 12rpx;
  margin: 10rpx 0 20rpx 0;
}
.com-card-loading .content-box .error-tip {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 600rpx;
  height: 220rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  flex-direction: column;
}
.com-card-loading .content-box .error-tip .error-text {
  font-size: 30rpx;
  line-height: 44rpx;
  color: #16a085;
  font-weight: bold;
  width: 100%;
  text-align: center;
  display: inline-block;
  margin-bottom: 20rpx;
}
.com-card-loading .content-box .error-tip .error-text .error-icon {
  display: inline-block;
  vertical-align: -6rpx;
  margin-right: 6rpx;
}
.com-card-loading .content-box .loading-tip {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 300rpx;
  height: 260rpx;
}
.com-card-loading .content-box .loading-tip .loading-icon {
  position: absolute;
  margin: auto;
  left: 0;
  right: 0;
  top: 0;
  width: 120rpx;
  height: 120rpx;
  transform: rotate(0deg);
  animation: circleAns 6s linear 0s infinite;
}
@keyframes circleAns {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(-720deg);
  }
}
.com-card-loading .content-box .loading-tip .loading-face-icon {
  position: absolute;
  margin: auto;
  left: 0;
  right: 0;
  top: 15rpx;
  width: 90rpx;
  height: 90rpx;
}
.com-card-loading .content-box .loading-tip .loading-text {
  font-size: 30rpx;
  line-height: 44rpx;
  color: #16a085;
  font-weight: bold;
  width: 100%;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 86rpx;
  padding-left: 0.9em;
}
